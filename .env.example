# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=insurance_service
DB_USERNAME=insurance_app
DB_PASSWORD=insurance_app_password

# Auth0 Configuration
AUTH0_ENABLED=false
AUTH0_DOMAIN=your-domain.auth0.com
AUTH0_API_TOKEN=your-api-token
AUTH0_CLIENT_ID=your-client-id
